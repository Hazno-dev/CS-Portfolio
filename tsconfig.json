{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@projects/*": [
        "src/content/projects/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
			"@mdx/*": [
				"src/components/mdx/*"
			]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}