---
import type { CollectionEntry } from 'astro:content';
import Text from '../text/text.astro';
import { Icon } from 'astro-icon/components';
import IconButton from '@components/core/image/icon-button.astro';
interface Props {
	project: CollectionEntry<'project'>;
}

const { project } = {
	project: Astro.props.project
};

const contributions = project.data.projectInfo.contributions;
---

<ul class="flex flex-col text-ellipsis ">
	{
		contributions.map((contribution) => (
			<li>
				<Text type="paragraph" class="!mb-0">
					{contribution.description}
				</Text>
			</li>
		))
	}
</ul>
