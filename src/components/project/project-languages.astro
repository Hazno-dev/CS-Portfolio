---
import type { CollectionEntry } from 'astro:content';
import TextInlineList from '@components/text/TextInlineList.astro';
interface Props {
	project: CollectionEntry<'project'>;
	class?: string;
}

const { project } = {
	project: Astro.props.project
};

const languages = project.data.projectInfo.languages;
const hasLanguages = languages && languages.length > 0;
---

{hasLanguages && <TextInlineList contents={languages} customTag="text-tag-heading !mt-0" />}
