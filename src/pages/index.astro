---
import { getCollection, getEntry } from 'astro:content';
import ProjectRow from '../components/projectCard/projectRow.astro';
import RootLayout from '../layouts/structure/root.layout.astro';
import ProjectCard from '../components/card/projectCard.astro';

const allProjects = await getCollection('project', ({ data }) => {
	return data.visible === true;
});
---

<RootLayout>
	{allProjects.map((project) => <ProjectCard project={project} /> <ProjectCard project={project} />)}
</RootLayout>
